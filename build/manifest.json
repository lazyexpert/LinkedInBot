{
    "manifest_version": 2,

    "name": "LinkedInBot",
    "version": "1.0",
    "icons": {
        "16": "img/16x16.png",
        "32": "img/32x32.png",
        "48": "img/48x48.png",
        "128": "img/128x128.png"
    },
    "permissions": [
        "activeTab",
        "http://www.linkedin.com/*",
        "http://linkedin.com/*"
    ],
    "browser_action": {
        "default_title": "LinkedInBot",
        "default_icon": "img/48x48.png",
        "default_popup": "index.html"
    }
}
